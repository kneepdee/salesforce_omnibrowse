<apex:page standardController="OmniBrowseConfiguration__c">
  <apex:form >
    <apex:pageBlock title="New OmniBrowse Configuration" mode="edit">
    <apex:pageBlockButtons >
    <apex:commandButton action="{!save}" value="Save"/>
    </apex:pageBlockButtons>
    <apex:pageBlockSection title="Configuration Details" columns="2">
      <apex:inputField value="{!OmniBrowseConfiguration__c.SiteId__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.ApiToken__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.AppToken__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.ExternalIDFieldName__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.ComponentHeight__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.ModelName__c}"/>
      <apex:inputField value="{!OmniBrowseConfiguration__c.Active__c}"/>
    </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>